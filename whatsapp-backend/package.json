{
  "name": "superparty-whatsapp-backend",
  "version": "2.0.1",
  "description": "WhatsApp Backend for SuperParty on Railway",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prestart": "node build-guard.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "audit:dupes": "node scripts/audit-firestore-duplicates.js",
    "cleanup:dupes": "node scripts/cleanup-firestore-duplicates.js",
